package com.cognizant.memberService.entity;

import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

@Entity
@Table(name = "claims")
public class Claim {
	
	long id;
	String claimStatus;
	
	@ManyToOne
	@JoinColumn(name = "member_id")
	Member member;
	
	
	long policyId;
	
}
